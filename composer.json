{
    "name": "pherman/symfony-console-app",
    "license": "MIT",
    "type": "project",
    "description": "",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/flex": "^2.0",
        "symfony/console": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/http-kernel": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/yaml": "6.2.*",
        "symfony/dependency-injection": "6.2.*",
        "symfony/config": "6.2.*",
        "symfony/filesystem": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/property-access": "6.2.*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.2.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    }
}
